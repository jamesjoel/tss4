<div class="container">
	<table class="mt-4 table-dark table-hover table-bordered">
		<tr>
			<th>S.No.</th>
			<th>Name of Software</th>
			<th>Deliver to (Company, Person)</th>
			<th>Price</th>
		</tr>
		<% products.forEach(x, n){ %>
		<tr>
			<td><%= n+1 %></td>
			<td><%= x.name %></td>
			<td><%= x.company%></td>
			<td><%= x.price %></td>
		</tr>
		<% } %>
	</table>
</div>