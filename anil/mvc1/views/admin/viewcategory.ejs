<div class="container">
	<div class="row">
		<div class="col-md-6 offset-md-3 text-center mt-4">
			<h2>All Categories in sequence</h2>
		</div>
		<dir class="dark-box"></dir>
	</div>
	<dir class="row">
		<div class="col-md-8 offset-md-2">
			<table class="table table-dark table-hover table-bordered">
				<tr>
					<th>S.No.</th>
					<th>Name</th>
					<th>Edit</th>
					<th>Delete</th>
				</tr>
			<%	result.forEach(function(x, n){ %>
			<tr>
					<td><%= n+1 %></td>
					<td><%= x.name %></td>
					<td><a href="/admin/viewcategory/editcategory?id=<%= x._id %>" class="btn btn-info btn-border">Edit</a></td>
					<td><a href="/admin/viewcategory/delete?id=<%= x._id %>" class="btn btn-danger btn-border">Delete</a></td>
			</tr>
			<% }) %>
			</table>
		</div>
	</dir>	
</div>