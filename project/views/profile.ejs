<div class="container">
	<div class="row">
		<div class="col-md-8 offset-md-2">
			<h2>My Profile</h2>
			<Hr />
			<table class="table table-bordered table-hover">
				<tr>
					<td>Full Name</td>
					<td><%= result.full_name %></td>
				</tr>
				<tr>
					<td>Username/Email</td>
					<td><%= result.username %></td>
				</tr>
				<tr>
					<td>Contact</td>
					<td><%= result.contact %></td>
				</tr>
				<tr>
					<td>Address</td>
					<td><%= result.address %></td>
				</tr>
				<tr>
					<td>Gender</td>
					<td><%= result.gender %></td>
				</tr>
				<tr>
					<td>City</td>
					<td><%= result.city %></td>
				</tr>
			</table>
		</div>
	</div>
</div>